$(document).ready(
		function () {
			$.ajax ({

				url:'/deleted',
				type:'DELETE',
				error: function(data) {
					console.log("Error")
				},
				success : $(".deleteBtn").on("click", function (data) {
					data.forEach(function (element) {
						$('#task-list').remove('<button type="button" class="edit-button" >' + "Edit" + '</button>',
											   '<button type="button" class="edit-button deleteBtn" >' + "Delete" + '</button>',
											   '<input type="checkbox" />' + element.id + '<br />'
											  );
					})
					console.log("caca")
				})
				
		})
		

		

		
});