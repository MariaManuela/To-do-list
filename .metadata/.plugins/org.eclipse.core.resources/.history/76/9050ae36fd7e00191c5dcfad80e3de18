$(document).ready(
		function () {
			$('.deleteBtn').click(deleteForm);
			$.ajax ({
				url:'/deleted',
				type:'DELETE',
				error: function(data) {
					console.log("Error")
				},
				success : function (data) {
					data.forEach(function (element) {
						$('#task-list').remove();
					})
				
		});
		
		
		
		function deleteForm(data) {
				data.forEach(function (element) {
					$('#task-list').remove();
				});
		}
		
});