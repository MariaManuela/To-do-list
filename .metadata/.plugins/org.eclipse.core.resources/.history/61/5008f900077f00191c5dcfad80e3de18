$(document).ready(
		function () {
			var id = element.data('id');
			$.ajax ({
				url:'/deleted' + id,
				type:'DELETE',
				error: function(data) {
					console.log("Error")
				},
				success : $(".deleteBtn").on("click", function () {
				    
					console.log("caca")
				})
				
		})
		

		

		
});