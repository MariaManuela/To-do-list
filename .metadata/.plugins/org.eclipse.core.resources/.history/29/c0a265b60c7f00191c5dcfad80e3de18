  $(document).ready(function () {
        $("#deleteBtn").click(function () {
            var getId = $(this).attr("name");
            $.ajax({
                url: "/deleted",
                type: "DELETE",
                data: { id: getId },
                success: function () {
                    $('table #' + getId).remove();
                    alert("success");
                },
                error: function () {
                    alert("Fails");
                }

            });
        });
    });